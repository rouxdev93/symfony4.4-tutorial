{% extends 'base.html.twig' %}

{% block title %}Blog Index!{% endblock %}
{% block body %}
    {% for id, post  in posts %}
        <p><a href="{{ path('blog_show', {'id': id}) }}">POST ID - {{ id }}</a></p>
        <p>TITLE - {{ post.title }}</p>
        <p>TEXT - {{ post.text }}</p>
    {% endfor %}
{% endblock %}